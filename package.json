    "@vueuse/core": "^10.7.2",
    "animate.css": "^4.1.1",
    "axios": "^1.6.7",
    "d3": "^7.8.5",
    "dayjs": "^1.11.10",
    "floating-vue": "^5.2.2",
    "geolib": "^3.3.4",
    "keyboard-event-key-type": "^1.5.0",
    "lodash": "^4.17.21",
    "ol": "^9.1.0",
    "ol-contextmenu": "^5.4.0",
    "ol-ext": "^4.0.18",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "proj4": "^2.11.0",
    "uuid": "^9.0.1",
